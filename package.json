{
  "name": "whenthesilly.github.io",
  "version": "0.1.0",
  "description": "the silley site",
  "scripts": {
	"build": "del www && cmd /C \"set ELEVENTY_ENV=prod && eleventy\"",
	"serve": "del www && cmd /C \"set ELEVENTY_ENV=dev && eleventy --serve\"",
	"update": "git pull && npm i && npm run build",
	"build:sass:prod": "sass src/static/scss:public/static/css --style compressed",
	"build:eleventy:prod": "ELEVENTY_ENV=production eleventy",
	"build:prod": "npm run build:eleventy:prod & npm run build:sass:prod"
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whenthesilly/whenthesilly.github.io.git"
  },
  "keywords": [
    "website"
  ],
  "author": "silly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whenthesilly/whenthesilly.github.io/issues"
  },
  "homepage": "https://github.com/whenthesilly/whenthesilly.github.io#readme",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1"
  }
}
